CREATE TABLE IF NOT EXISTS pokemonCard (
    "id" TEXT,
    "name" TEXT,
    "supertype" TEXT,
    "subtypes" TEXT,
    "rules" TEXT,
    "set_id" TEXT, -- This column references the cardSet table
    "number" INT,
    "artist" TEXT,
    "rarity" TEXT,
    "legalities_unlimited" TEXT,
    "images_small" TEXT,
    "images_large" TEXT,
    "legalities_expanded" TEXT,
    "legalities_standard" TEXT,
    "regulationMark" TEXT,
    "level" INT,
    "hp" INT,
    "types" TEXT,
    "evolvesTo" TEXT,
    "attacks_name" TEXT,
    "attacks_cost" TEXT,
    "attacks_convertedEnergyCost" INT,
    "attacks_damage" TEXT,
    "attacks_text" TEXT,
    "weaknesses_type" TEXT,
    "weaknesses_value" TEXT,
    "retreatCost" TEXT,
    "convertedRetreatCost" INT,
    "flavorText" TEXT,
    "nationalPokedexNumbers" INT,
    "resistances_type" TEXT,
    "resistances_value" INT,
    "abilities_name" TEXT,
    "abilities_text" TEXT,
    "abilities_type" TEXT,
    "evolvesFrom" TEXT,
    
    -- Define the foreign key constraint
    FOREIGN KEY ("set_id") REFERENCES cardSet("id")
);